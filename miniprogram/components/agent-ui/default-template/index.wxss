/* components/agent-ui/collapsibleCard/index.wxss */
.default-template{
  position: relative;
  width: 670rpx;
  
  margin-left: 40rpx;
  margin-top: 30rpx;
}
.ai-robot {
  position: absolute;
  top: -55rpx;
  right: 0;
  width: 180rpx;
  height: 178rpx;
}
.content {
  /* background: #F7F8F9; */
  width: 670rpx;
  /* height: 624rpx; */
  border-radius: 30rpx;
  opacity: 1;
  padding: 30rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
  background: linear-gradient(77deg, #C5FCF6 10%, #F4D8EA 41%, #DEDDF8 70%, #D3DEF5 93%);
}

  
 
  
.content > .title {
  color: #2C2F3B;
  font-size: 40rpx;
  font-family: 'MySimSun';
}

.description {
  color: #9AA7B1;
  font-size: 30rpx;
  margin-top: 20rpx;
  line-height: 44rpx;
  width: 450rpx;
}
.question {
 
  margin-top: 30rpx;
  
 
}
.tips {
  color: #9AA7B1;
  font-size: 30rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 30rpx 0 40rpx 0;
}
.change-area {
  
}
.change-btn {
  width: 23rpx;
  height: 23rpx;
}
.change-btn-text {
  color: #2C2F3B;
  font-size: 30rpx;
  padding-left: 12rpx;
}
.question-area {
  
}
.question-item {
  width: 610rpx;
  height: 82rpx;
  border-radius: 20rpx;
  background: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
  margin-bottom: 30rpx;
  box-sizing: border-box;
}
.question-item-title {
  color: #2C2F3B;
  font-size: 30rpx;
}
.question-item-arrow {
  width: 30rpx;
  height: 30rpx;
}
.func-list {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}
.func-item {
  width: 200rpx;
  height: 200rpx;
  border-radius: 30rpx;
  background: #F7F8F9;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.func-item-img {
  width: 60rpx;
  height: 60rpx;
}
.func-item-text {
  color: #2C2F3B;
  font-size: 30rpx;
  margin-top: 20rpx; 
}