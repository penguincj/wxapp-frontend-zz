{
  "pages": [
    "pages/index/index",
    "pages/ai-camera/index",
    "pages/take-photo/index",
    "pages/citylist/index",
    "pages/login/index",
    "pages/profile/index",
    "pages/personalinfo/index",
    "pages/setting/index",
    "pages/feedback/index",
    "pages/guidepage/index",
    "pages/searchpage/index",
    "pages/agent/index"
  ],
  "subpackages": [
    {
      "root": "pages/personal",
      "pages": [
        "looklist/index",
        "collectionlist/index",
        "likelist/index"
      ]
    }
  ],
  "usingComponents": {
    "agent-ui": "/components/agent-ui"
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "disableScroll": true
  },
  "style": "v2",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "requiredPrivateInfos": [
    "getLocation"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    },
    "scope.camera": {
      "desc": "需要使用您的相机"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "requiredBackgroundModes": ["location"],
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "pagePath": "pages/profile/index",
        "text": "我的"
      }
    ]
  }
}
